<tr class="has-data-in-range-{{hasDataInRange}} tooltips"   data-indicator="{{ indicator }}"  data-bbox="{{ bbox }}">
	<td><div class="close-acitve-plot-component icon-cross"></div></td>
	<td>
		<b>Indicator:</b> <span>{{indicatorObj.name}} - {{indicatorObj.tags.region}}</span><br>
		<b>BBox:</b> <span>{{#if bbox}} {{truncate bbox 10}} {{else}} Not set {{/if}}</span>
	</td>
	<td>
		<div class=" icon-data"></div>
	</td>
</tr>