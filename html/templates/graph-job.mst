<div class="graph-job">
	<div>
		{{#if saved}}
			<div class="save" ></div>
		{{else}}
			<div class="unsave" ></div>
		{{/if}}
		<div>
			<h2 class="title singleLine" title="{{title}}">{{title}}</h2>
		</div>
	</div>
	<div>
		<div class="loading-image"></div><div>Status: Complete</div>
	</div>
</div>